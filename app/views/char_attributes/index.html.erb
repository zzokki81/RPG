<p id="notice"><%= notice %></p>
<h1>Char Attributes</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @char_attributes.each do |char_attribute| %>
      <tr>
        <td><%= char_attribute.name %></td>
        <td><%= char_attribute.value %></td>
        <td><%= link_to 'Show', char_attribute %></td>
        <td><%= link_to 'Edit', edit_char_attribute_path(char_attribute) %></td>
        <td><%= link_to 'Destroy', char_attribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<div id="attributes">
 <%= link_to 'New Char Attribute', new_char_attribute_path, remote: true %>
</div>
